#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/landing-hub/node_modules/.pnpm/jest-expo@49.0.0_@babel+core@7.28.4_jest@27.5.1_react@18.2.0/node_modules/jest-expo/bin/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/jest-expo@49.0.0_@babel+core@7.28.4_jest@27.5.1_react@18.2.0/node_modules/jest-expo/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/jest-expo@49.0.0_@babel+core@7.28.4_jest@27.5.1_react@18.2.0/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/landing-hub/node_modules/.pnpm/jest-expo@49.0.0_@babel+core@7.28.4_jest@27.5.1_react@18.2.0/node_modules/jest-expo/bin/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/jest-expo@49.0.0_@babel+core@7.28.4_jest@27.5.1_react@18.2.0/node_modules/jest-expo/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/jest-expo@49.0.0_@babel+core@7.28.4_jest@27.5.1_react@18.2.0/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-expo/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-expo/bin/jest.js" "$@"
fi
