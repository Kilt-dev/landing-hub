#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/landing-hub/node_modules/.pnpm/expo@54.0.7_@babel+core@7.2_c02f65c91415b73b329464c60c62c22d/node_modules/expo/bin/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/expo@54.0.7_@babel+core@7.2_c02f65c91415b73b329464c60c62c22d/node_modules/expo/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/expo@54.0.7_@babel+core@7.2_c02f65c91415b73b329464c60c62c22d/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/landing-hub/node_modules/.pnpm/expo@54.0.7_@babel+core@7.2_c02f65c91415b73b329464c60c62c22d/node_modules/expo/bin/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/expo@54.0.7_@babel+core@7.2_c02f65c91415b73b329464c60c62c22d/node_modules/expo/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/expo@54.0.7_@babel+core@7.2_c02f65c91415b73b329464c60c62c22d/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/cli" "$@"
else
  exec node  "$basedir/../expo/bin/cli" "$@"
fi
