#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/landing-hub/node_modules/.pnpm/react-native@0.72.10_@babel_1fa22553d9d8f2368d044b1b0de29541/node_modules/react-native/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/react-native@0.72.10_@babel_1fa22553d9d8f2368d044b1b0de29541/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/landing-hub/node_modules/.pnpm/react-native@0.72.10_@babel_1fa22553d9d8f2368d044b1b0de29541/node_modules/react-native/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/react-native@0.72.10_@babel_1fa22553d9d8f2368d044b1b0de29541/node_modules:/mnt/d/landing-hub/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
