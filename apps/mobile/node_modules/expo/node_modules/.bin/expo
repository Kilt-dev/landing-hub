#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/landinghub-iconic/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.4/node_modules/expo/bin/node_modules:/mnt/f/landinghub-iconic/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.4/node_modules/expo/node_modules:/mnt/f/landinghub-iconic/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.4/node_modules:/mnt/f/landinghub-iconic/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/landinghub-iconic/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.4/node_modules/expo/bin/node_modules:/mnt/f/landinghub-iconic/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.4/node_modules/expo/node_modules:/mnt/f/landinghub-iconic/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.4/node_modules:/mnt/f/landinghub-iconic/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli" "$@"
else
  exec node  "$basedir/../../bin/cli" "$@"
fi
