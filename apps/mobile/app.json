{
  "expo": {
    "name": "LandingHub Mobile",
    "slug": "landinghub-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./index.js",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
